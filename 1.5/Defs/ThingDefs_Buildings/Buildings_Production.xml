<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="BuildingBase">
		<defName>Owl_WoodDryingRack</defName>
		<label>wood drying rack</label>
		<description>Green lumber can be stored here where it can air dry. But it is susceptible to bad weather.</description>    
		<thingClass>Building</thingClass>
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<texPath>Things/Building/Production/wood_drying_rack</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(2,2)</drawSize>
			<damageData>
				<rectN>(0.125,0.296875,0.734375,0.421875)</rectN>
				<rectS>(0.125,0.296875,0.734375,0.421875)</rectS>
				<rectE>(0.234375,0.296875,0.5,0.546875)</rectE>
				<rectW>(0.234375,0.296875,0.5,0.546875)</rectW>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
			<shadowData>
				<volume>(0.5, 0.5, 0.5)</volume>
				<offset>(0,0,-0.25)</offset>
			</shadowData>
		</graphicData>
		<uiIconPath>Things/Building/Production/wood_drying_rack_north</uiIconPath>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>RawWoody</li>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>10</costStuffCount>
		<altitudeLayer>Building</altitudeLayer>
		<fillPercent>0.3</fillPercent>
		<castEdgeShadows>true</castEdgeShadows>
		<staticSunShadowHeight>0</staticSunShadowHeight>
		<useHitPoints>True</useHitPoints>
		<statBases>
			<Mass>4</Mass>
			<MaxHitPoints>75</MaxHitPoints>
			<WorkToBuild>500</WorkToBuild>
			<Flammability>1.0</Flammability>
		</statBases>
		<designationCategory>Production</designationCategory>
		<passability>PassThroughOnly</passability>
		<pathCost>70</pathCost>
		<modExtensions>
			<li MayRequire="owlchemist.perspectivebuildings" Class="Perspective.Offsetter" />
		</modExtensions>
		<comps>
			<li Class="ProcessorFramework.CompProperties_Processor">
				<showProductIcon>false</showProductIcon>
				<capacity>75</capacity>
				<independentProcesses>true</independentProcesses>
				<parallelProcesses>false</parallelProcesses>
				<processes>
					<li>Owl_WoodLog</li>
				</processes>
			</li>
			<li Class="CompProperties_Forbiddable"/>
		</comps>
		<tickerType>Rare</tickerType>
		<rotatable>false</rotatable>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
		<building>
			<uninstallWork>125</uninstallWork>
			<buildingTags>
				<li>Production</li>
			</buildingTags>
		</building>
	</ThingDef>
	
	<ProcessorFramework.ProcessDef>
		<defName>Owl_WoodLog</defName>
		<thingDef>WoodLog</thingDef>
		<ingredientFilter>
			<thingDefs>
				<li>Owl_GreenLumber</li>
			</thingDefs>
		</ingredientFilter>
		<processDays>0.67</processDays>
		<temperatureIdeal>10~100</temperatureIdeal>
		<temperatureSafe>-50~200</temperatureSafe>
		<sunFactor>0.4~1.0</sunFactor>
		<rainFactor>1~0</rainFactor>
		<snowFactor>1~0</snowFactor>
		<windFactor>0.8~1.0</windFactor>
		<filledGraphicSuffix>_full</filledGraphicSuffix>
	</ProcessorFramework.ProcessDef>
	
	<ThingDef ParentName="BuildingBase">
		<defName>Owl_CarpentersTrestle</defName>
		<label>carpentry trestle</label>
		<description>A pair of trestles for basic woodworking tasks.</description>    
		<thingClass>Building_WorkTable</thingClass>
		<graphicData>
			<texPath>Things/Building/Production/carpenters_trestle</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(2.9,1.1)</drawSize>
			<drawOffset>(0,0,0.1)</drawOffset>
		</graphicData>
		<castEdgeShadows>true</castEdgeShadows>
		<staticSunShadowHeight>0.20</staticSunShadowHeight>
		<uiIconPath>Things/Building/Production/carpenters_trestle_north</uiIconPath>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>RawWoody</li>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>20</costStuffCount>
		<costList>
			<Steel>5</Steel>
		</costList>
		<altitudeLayer>Building</altitudeLayer>
		<fillPercent>0.2</fillPercent>
		<useHitPoints>True</useHitPoints>
		<statBases>
			<Mass>3</Mass>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToBuild>2000</WorkToBuild>
			<Flammability>0.75</Flammability>
			<Cleanliness>-5</Cleanliness>
			<WorkTableWorkSpeedFactor>0.6</WorkTableWorkSpeedFactor>
		</statBases>
		<size>(3,1)</size>
		<designationCategory>Production</designationCategory>
		<passability>PassThroughOnly</passability>
		<pathCost>70</pathCost>
		<hasInteractionCell>True</hasInteractionCell>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<surfaceType>Item</surfaceType>
		<inspectorTabs>
			<li>ITab_Bills</li>
		</inspectorTabs>
		<building>
			<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
			<uninstallWork>500</uninstallWork>
			<buildingTags>
				<li>Production</li>
			</buildingTags>
			<relatedBuildCommands>
				<li>Owl_WoodDryingRack</li>
			</relatedBuildCommands>
		</building>
		<comps>
			<li Class="CompProperties_AffectedByFacilities">
				<linkableFacilities>
					<li>ToolCabinet</li>
				</linkableFacilities>
			</li>
		</comps>
		<placeWorkers>
			<li>PlaceWorker_ShowFacilitiesConnections</li>
			<!--<li>PlaceWorker_PreventInteractionSpotOverlap</li>-->
		</placeWorkers>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
	</ThingDef>	
	
	<ThingDef ParentName="BuildingBase">
		<defName>TM3_ElectricTableSaw</defName>
		<label>electric table saw</label>
		<description>An electric table saw for woodworking tasks.</description>    
		<thingClass>Building_WorkTable_HeatPush</thingClass>
		<graphicData>
			<texPath>Things/Building/Production/TableWoodworking</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(3.5,1.5)</drawSize>
			<drawOffset>(0,0,0.1)</drawOffset>
		</graphicData>
		<castEdgeShadows>true</castEdgeShadows>
		<staticSunShadowHeight>0.20</staticSunShadowHeight>
		<uiIconPath>Things/Building/Production/TableWoodworking_north</uiIconPath>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>75</costStuffCount>
		<costList>
			<Steel>50</Steel>
			<ComponentIndustrial>2</ComponentIndustrial>
		</costList>
		<altitudeLayer>Building</altitudeLayer>
		<fillPercent>0.5</fillPercent>
		<useHitPoints>True</useHitPoints>
		<statBases>
			<Mass>3</Mass>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToBuild>2000</WorkToBuild>
			<Flammability>0.75</Flammability>
			<Cleanliness>-3</Cleanliness>
			<!--<WorkTableWorkSpeedFactor>1</WorkTableWorkSpeedFactor>-->
		</statBases>
		<size>(3,1)</size>
		<designationCategory>Production</designationCategory>
		<passability>PassThroughOnly</passability>
		<pathCost>70</pathCost>
		<hasInteractionCell>True</hasInteractionCell>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<surfaceType>Item</surfaceType>
		<constructionSkillPrerequisite>4</constructionSkillPrerequisite>
		<inspectorTabs>
			<li>ITab_Bills</li>
		</inspectorTabs>
		<building>
			<workTableRoomRole>Workshop</workTableRoomRole>
			<workTableNotInRoomRoleFactor>0.8</workTableNotInRoomRoleFactor>
			<spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
			<uninstallWork>500</uninstallWork>
			<heatPerTickWhileWorking>0.08</heatPerTickWhileWorking>
			<buildingTags>
				<li>Production</li>
			</buildingTags>
			<relatedBuildCommands>
				<li>Owl_WoodDryingRack</li>
			</relatedBuildCommands>
		</building>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<shortCircuitInRain>true</shortCircuitInRain>
				<basePowerConsumption>200</basePowerConsumption>
			</li>
			<li Class="CompProperties_Flickable"/>
			<li Class="CompProperties_Breakdownable"/>
			<li Class="CompProperties_AffectedByFacilities">
				<linkableFacilities>
					<li>ToolCabinet</li>
					<li MayRequire="OskarPotocki.VFE.Medieval2">VFEM2_ChiselRack</li>
					<!--<li MayRequire="OskarPotocki.VFE.Medieval2">VFEM2_ArtToolStand</li>
					<li MayRequire="OskarPotocki.VFE.Medieval2">VFEM2_StonePolisher</li>-->
				</linkableFacilities>
			</li>
		</comps>
		<placeWorkers>
			<li>PlaceWorker_ShowFacilitiesConnections</li>
			<!--<li>PlaceWorker_PreventInteractionSpotOverlap</li>-->
		</placeWorkers>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
		<researchPrerequisites>
			<li>Electricity</li>
		</researchPrerequisites>
	</ThingDef>	
</Defs>